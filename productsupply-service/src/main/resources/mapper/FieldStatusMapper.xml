<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.zq.dao.FieldStatusMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.zq.pojo.FieldStatus">
        <id column="id" property="id" />
        <result column="proc_def_key" property="procDefKey" />
        <result column="task_def_id" property="taskDefId" />
        <result column="field_name" property="fieldName" />
        <result column="status" property="status" />
        <result column="isList" property="islist" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, proc_def_key, task_def_id, field_name, status, isList
    </sql>

</mapper>
